{
  "name": "@tson/parse",
  "version": "1.0.5",
  "description": "TSON (Terse Single Object Notation) parser",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devdigital/tson.git",
    "directory": "packages/parse"
  },
  "homepage": "https://github.com/devdigital/tson/tree/master/packages/parse#readme",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "build": "tsup && yarn create-package",
    "watch": "tsup --watch",
    "create-package": "mkdir -p ./output && cp -R ./dist/. ./output/dist && cp package.json ./output"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@utilz/deepmerge": "^0.1.21",
    "@utilz/types": "^0.1.19"
  },
  "devDependencies": {
    "tsup": "^7.3.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  }
}
